{
  "name": "@wottpal/nextjs-starter",
  "description": "A minimal and opinionated Next.js template.",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22"
  },
  "engineStrict": true,
  "packageManager": "bun@1.2.2",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "bun run clean && next build && bun run typecheck",
    "build:standalone": "NEXT_OUTPUT=standalone next build",
    "build:standalone-and-copy": "NEXT_OUTPUT=standalone next build && cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "analyze": "ANALYZE=true bun run build",
    "clean": "rm -rf .next && rm -rf ./content-collections/.content-collections",
    "start": "next start",
    "start:standalone": "bun --bun .next/standalone/server.js",
    "preview": "next build && next start",
    "lint": "biome check . --files-ignore-unknown=true && prettier . --check",
    "lint:fix": "biome check . --write --files-ignore-unknown=true && prettier . --write",
    "typecheck": "tsc --pretty --noEmit",
    "typesync": "typesync --dry",
    "postinstall": "biome check package.json --write > /dev/null 2>&1 && next telemetry disable > /dev/null 2>&1",
    "update": "bunx taze --interactive --write --install --includeLocked --exclude \"tailwindcss,tailwind-merge\""
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,jsx,tsx,json,jsonc,css}": "biome check --write --no-errors-on-unmatched",
    "*.{md,mdx,html,yml,yaml,sass,scss}": "prettier . --write"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx --bun nano-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@t3-oss/env-nextjs": "^0.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "cookies-next": "^5.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dedent": "^1.5.3",
    "deepmerge": "^4.3.1",
    "embla-carousel-react": "^8.5.2",
    "input-otp": "^1.4.2",
    "isbot": "^5.1.22",
    "lucide-react": "^0.475.0",
    "next": "canary",
    "next-intl": "^3.26.4",
    "next-navigation-guard": "^0.1.2",
    "next-themes": "^0.4.4",
    "posthog-js-lite": "^3.3.0",
    "posthog-node": "^4.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.1",
    "rehype-stringify": "^10.0.1",
    "sharp": "^0.33.5",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@content-collections/cli": "^0.1.6",
    "@content-collections/core": "^0.8.0",
    "@content-collections/mdx": "^0.2.0",
    "@content-collections/next": "^0.2.4",
    "@next/bundle-analyzer": "canary",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/negotiator": "~0.6.3",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "~10.0.0",
    "autoprefixer": "^10.4.20",
    "nano-staged": "^0.8.0",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "rehype-slug": "^6.0.0",
    "remark-preset-lint-recommended": "^7.0.1",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typesync": "^0.14.1"
  },
  "trustedDependencies": ["@biomejs/biome", "esbuild", "sharp"]
}
