{
  "name": "@wottpal/nextjs-starter",
  "description": "A minimal and opinionated Next.js template.",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "biome check . --files-ignore-unknown=true && prettier . --check",
    "lint:fix": "biome check . --write --files-ignore-unknown=true && prettier . --write",
    "update": "npm-check-updates --interactive --root --format group --install always -x '@types/node'",
    "typecheck": "tsc --pretty --noEmit",
    "typesync": "typesync && bun install"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,jsx,tsx,json,jsonc}": "biome check --write --no-errors-on-unmatched",
    "*.{md,mdx,html,css,sass,scss,yml,yaml}": "prettier . --write"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx nano-staged"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "bun@1.1.26",
  "dependencies": {
